sudo yum update -y
sudo yum install python3 -y
sudo pip3 install --upgrade streamlink
sudo pip3 install --upgrade google-auth-httplib2 google-auth-oauthlib
sudo wget https://animeshxd.github.io/private/gdrive-upload.zip
unzip gdrive-upload.zip
cd gdrive-upload/
sudo pip3 install -r requirements.txt
cd

streamlink --http-header "Referer=RefererURL" "hlsvariant://list.m3u8XHR" best -o output.ts

python3 upload.py --noauth_local_webserver -i ~/output.ts -o output.ts -f Doraemon
